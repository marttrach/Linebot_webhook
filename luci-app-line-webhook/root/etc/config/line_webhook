config line_webhook 'main'
option enabled '0'
option port '5000'
option bind_address '0.0.0.0'
option log_level 'info'
option access_token ''
option channel_secret ''
option use_tls '0'
option tls_cert '/etc/ssl/line_webhook/server.crt'
option tls_key '/etc/ssl/line_webhook/server.key'
option processor 'echo'
	option grafana_user_id ''
	option grafana_secret ''
option remote_api_url ''
option remote_api_key ''
option remote_api_model ''
option remote_api_timeout '15'
option openclaw_bridge_url 'http://127.0.0.1:5001/message'
option openclaw_timeout '60'
option anythingllm_url ''
option anythingllm_api_key ''
option anythingllm_mode 'chat'
option anythingllm_timeout '60'
option n8n_webhook_url ''
option n8n_webhook_secret ''
option n8n_timeout '10'
